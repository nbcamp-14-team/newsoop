<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#A2133B" /> <!-- 배경 색상: 흰색 -->
            <stroke android:color="#909790" android:width="2dp" /> <!-- 테두리 색상: 흰색, 두께: 2dp -->
            <corners android:radius="30dp" /> <!-- 둥근 모서리 반경을 조절 -->
        </shape>
    </item>
</layer-list>
